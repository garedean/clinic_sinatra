<h1 class="page-header text-center">Add a doctor to the clinic roster</h1>


<form id="form_id" action="/doctors" method="post">
  <div class="form-group">
    <label for="first_name">First Name</label>
    <input required type="text" name="first_name" id="first_name" class="form-control">
  </div>

  <div class="form-group">
    <label for="last_name">last Name</label>
    <input required type="text" name="last_name" id="last_name" class="form-control">
  </div>

  <button type="submit" name="add_doctor" class="btn-primary">Submit</button>

</form>
